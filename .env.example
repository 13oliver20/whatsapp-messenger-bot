# Credenciales de Twilio
# Obtén estas credenciales en: https://console.twilio.com

# Account SID (ejemplo: ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx)
TWILIO_ACCOUNT_SID=tu_account_sid_aqui

# Auth Token (ejemplo: xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx)
TWILIO_AUTH_TOKEN=tu_auth_token_aqui

# Número de WhatsApp de Twilio (incluye código de país, ejemplo: +14155238886)
TWILIO_WHATSAPP_NUMBER=+14155238886

# Puerto del servidor (opcional, por defecto 3000)
PORT=3000
